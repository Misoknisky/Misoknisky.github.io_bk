\section{Matrix}
\[ 
\begin{pmatrix}
a&amp;b&amp;c \\ 123&amp;234&amp;34
\end{pmatrix}^T \quad
\begin{bmatrix}
a&amp;b&amp;c \\ 123&amp;234&amp;34
\end{bmatrix} \quad
\begin{Bmatrix}
a&amp;b&amp;c \\ 123&amp;234&amp;34
\end{Bmatrix}
\begin{vmatrix}
a&amp;b&amp;c \\ 123&amp;234&amp;34
\end{vmatrix}
\begin{Vmatrix}
a&amp;b&amp;c \\ 123&amp;234&amp;34
\end{Vmatrix}
\]
$( \begin{smallmatrix}
a&amp;b \\ c&amp;b
\end{smallmatrix} )$
\begin{multline}
\sum_{i=1}^{i=n} a_i= 1 \\
a= b +c \\
c=a^2 + b^2 
\end{multline}
\begin{multline*}
\sum_{i=1}^{i=n} a_i= 1 \\
a= b +c \\
c=a^2 + b^2 
\end{multline*}
\[ \begin{aligned}
x ={}&amp; a+b+c+{} \\
&amp;d+e+f+g 
\end{aligned} \]
\begin{gather}
x=a+b+c \\
c=1+x+y \\
y=A^T + B_2
\end{gather}
\begin{gather*}
x=a+b+c \\
c=1+x+y \\
y=A^T + B_2
\end{gather*}
\[
y= \begin{cases}
-x,x \leq 0 \\
x,x&gt;0
\end{cases}
\]
\end{document}
